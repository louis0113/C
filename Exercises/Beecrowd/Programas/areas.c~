#include <stdio.h>
#include <math.h>
#define PI 3.14159

float areaTriangulo(float x, float y){

  return (x * y) / 2; 

}

float areaCirculo(float raio){

  return PI * pow(raio,2);

}

float areaTrapezio(float b1, float b2, float h){

  return ((b1 + b2) * h) / 2;


}


int main (void){

  float a,b,c;

  scanf("%f %f %f", &a, &b , &c);
  return 0;

}
